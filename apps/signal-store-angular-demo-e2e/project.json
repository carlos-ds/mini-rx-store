{
    "name": "signal-store-angular-demo-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/signal-store-angular-demo-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nx/cypress:cypress",
            "options": {
                "cypressConfig": "apps/signal-store-angular-demo-e2e/cypress.json",
                "devServerTarget": "signal-store-angular-demo:serve:development",
                "testingType": "e2e"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "signal-store-angular-demo:serve:production"
                },
                "ci": {
                    "devServerTarget": "signal-store-angular-demo:serve-static"
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/signal-store-angular-demo-e2e/**/*.{js,ts}"]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["signal-store-angular-demo"]
}
